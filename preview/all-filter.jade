												option(value="2002") 2002
												option(value="2001") 2001
												option(value="2000") 2000
												option(value="1995") 1995
												option(value="1990") 1990
												option(value="1985") 1985
												option(value="1980") 1980
										.ib-item-wrap-fm.ib-sp-fm to
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMax", name="maxyear")
												option(value="any") Any
												option(value="---", disabled="") ---
												option(value="2020") 2020
												option(value="2019") 2019
												option(value="2018") 2018
												option(value="2017") 2017
												option(value="2016") 2016
												option(value="2015") 2015
												option(value="2014") 2014
												option(value="2013") 2013
												option(value="2012") 2012
												option(value="2011") 2011
												option(value="2010") 2010
												option(value="2009") 2009
												option(value="2008") 2008
												option(value="2007") 2007
												option(value="2006") 2006
												option(value="2005") 2005
												option(value="2004") 2004
												option(value="2003") 2003
												option(value="2002") 2002
												option(value="2001") 2001
												option(value="2000") 2000
												option(value="1995") 1995
												option(value="1990") 1990
												option(value="1985") 1985
												option(value="1980") 1980
							
							//WATERFRONT DESCRIPTION
							.ib-item-collapse
								h2.ib-header-collapse Waterfront Description
								.ib-body-collpase
									.ib-wrap-fm
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-flex-waterfront-switch")
												option(value="any") Any
												option(value="---", disabled="") ---
												option(value="bay") Bay Front
												option(value="canal") Canal
												option(value="fixed-bridge") Fixed Bridge
												option(value="intracoastal") Intracoastal
												option(value="lake") Lake Front
												option(value="point-lot") Point Lot
												option(value="river") River Front
							
							//FEATURES
							.ib-item-collapse
								h2.ib-header-collapse Features
								.ib-body-collpase
									.ib-wrap-fm
										ul.ib-wrap-fm.ib-cl-2
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="pool", id="s_amenity_pool")
												label(for="s_amenity_pool") Swimming Pool
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="golf", id="s_amenity_golf")
												label(for="s_amenity_golf") Golf Course
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="tennis", id="s_amenity_tennis")
												label(for="s_amenity_tennis") Tennis Courts
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="gated_community", id="_amenity_gated_community")
												label(for="_amenity_gated_community") Gated Community
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="penthouse", id="s_amenity_penthouse")
												label(for="s_amenity_penthouse") Penthouse
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="water_front" id="s_amenity_water_front")
												label(for="s_amenity_water_front") Water Front
							
					
					//Footer modal
					.ib-footer-modal-filters-mobile
						.ib-group-buttons-content
							button#ib-apply-save-btn Save Search
							button#ib-apply-filters-btn View 
								span 50 
								|Properties

		
