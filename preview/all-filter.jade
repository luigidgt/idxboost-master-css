doctype html
html(lang='en')
	head
		title Page Results | FlexIdx
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no")
		meta(name="apple-mobile-web-app-capable", content="yes")
		meta(name="apple-mobile-web-app-status-bar-style", content="#4b4b4b")
		meta(name="theme-color", content="#4b4b4b")
		meta(name="msapplication-navbutton-color", content="#4b4b4b")
		link(rel="stylesheet", href="../css/main.css")
		//Font
		link(rel="stylesheet", href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800&subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese")

	body
		main

			.ib-modal-filters-mobile
				.ib-content-modal-filters-mobile

					//Header modal
					.ib-header-modal-filters-mobile
						//img(src="../images/logo.png")
						//img(src="http://idxb073.staging.wpengine.com/wp-content/uploads/2018/06/logo_roberto_black.png")
						img(src="http://idxb084.staging.wpengine.com/wp-content/uploads/2018/08/logohome-1.png")
						button.ib-close-modal-filters-mobile: span Close
					
					//Boby modal
					.ib-body-modal-filters-mobile
						.ib-filter-autocomplete
							.ib-filter-content-autocomplete
								input(type="search", placeholder="Enter address, city, zip or MLS").ib-autocomplete-input#ib-autocomplete-input
								span.ib-icon-search-ligth
								span.ib-line-form
								button#ib-close-search-modal Close
								button#ib-active-autocomplete
						
						.ib-wrap-collapse
							//PRICE RANGE
							.ib-item-collapse
								h2.ib-header-collapse Price Range
								.ib-body-collpase
									.ib-wrap-fm
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="0") 0
												option(value="---", disabled="") ---
												option(value="100") $100
												option(value="200") $200
												option(value="300") $300
												option(value="90,000,000") $90,000,000
											span.ib-label-wrap-fm Min Price
										.ib-item-wrap-fm.ib-sp-fm.ib-sp-fm-tp to
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="any") Any
												option(value="---", disabled="") ---
												option(value="100") $100
												option(value="200") $200
												option(value="300") $300
												option(value="90,000,000") $90,000,000
											span.ib-label-wrap-fm Max Price

							//BEDROOMS
							.ib-item-collapse
								h2.ib-header-collapse Bedrooms
								.ib-body-collpase
									ul.ib-wrap-fm
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="0", id="bd_any" name="radio_bedrooms", checked)
											label(for="bd_any") Any
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="1", id="bd_1" name="radio_bedrooms")
											label(for="bd_1") 1+
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="2", id="bd_2" name="radio_bedrooms")
											label(for="bd_2") 2+
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="3", id="bd_3" name="radio_bedrooms")
											label(for="bd_3") 3+
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="4", id="bd_4" name="radio_bedrooms")
											label(for="bd_4") 4+
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="5", id="bd_5" name="radio_bedrooms")
											label(for="bd_5") 5+
									
									.ib-line-or
										span or

									ul.ib-wrap-fm
										li.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="from") From
												option(value="---", disabled="") ---
												option(value="studio") Studio
												option(value="1") 1
												option(value="2") 2
												option(value="3") 3
												option(value="4") 4
												option(value="5") 5
										li.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="to") to
												option(value="---", disabled="") ---
												option(value="studio") Studio
												option(value="1") 1
												option(value="2") 2
												option(value="3") 3
												option(value="4") 4
												option(value="5") 5

							//BADTHROOMS
							.ib-item-collapse
								h2.ib-header-collapse Bathrooms
								.ib-body-collpase
									ul.ib-wrap-fm
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="0", id="bs_any", name="radio_bathrooms", checked)
											label(for="bs_any") Any
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="1", id="bs_1", name="radio_bathrooms")
											label(for="bs_1") 1+
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="2", id="bs_2", name="radio_bathrooms")
											label(for="bs_2") 2+
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="3", id="bs_3", name="radio_bathrooms")
											label(for="bs_3") 3+
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="4", id="bs_4", name="radio_bathrooms")
											label(for="bs_4") 4+
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="5", id="bs_5", name="radio_bathrooms")
											label(for="bs_5") 5+
							
							//TYPE
							.ib-item-collapse
								h2.ib-header-collapse Type
								.ib-body-collpase
									.ib-wrap-fm
										ul.ib-wrap-fm.ib-cl-2
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="pool", id="_pt_1")
												label(for="_pt_1") Single Family Homes
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="golf", id="_pt_2")
												label(for="_pt_2") Condominiums
											li.ib-item-wrap-fm.ib-btn-chk-fm
												input(type="checkbox", value="tennis", id="_pt_3")
												label(for="_pt_3") Townhouses

							//PAKING SPACES
							.ib-item-collapse
								h2.ib-header-collapse Parking Spaces
								.ib-body-collpase
									ul.ib-wrap-fm
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="0", id="pk_any", name="radio_parking", checked)
											label(for="pk_any") Any
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="1", id="pk_1", name="radio_parking")
											label(for="pk_1") 1
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="2", id="pk_2", name="radio_parking")
											label(for="pk_2") 2
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="3", id="pk_3", name="radio_parking")
											label(for="pk_3") 3
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="4", id="pk_4", name="radio_parking")
											label(for="pk_4") 4
										li.ib-item-wrap-fm.ib-btn-chk-fm
											input(type="radio", value="5", id="pk_5", name="radio_parking")
											label(for="pk_5") 5+
							
							//LIVING SIZE
							.ib-item-collapse
								h2.ib-header-collapse Living Size
								.ib-body-collpase
									.ib-wrap-fm
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="0") 0 Sq.Ft.
												option(value="---", disabled="") ---
												option(value="100") 100 Sq.Ft.
												option(value="200") 200 Sq.Ft.
												option(value="1000") 1000 Sq.Ft.
											span.ib-label-wrap-fm Min size
										.ib-item-wrap-fm.ib-sp-fm.ib-sp-fm-tp to
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="any") Any
												option(value="---", disabled="") ---
												option(value="100") 100 Sq.Ft.
												option(value="200") 200 Sq.Ft.
												option(value="1000") 1000 Sq.Ft.
											span.ib-label-wrap-fm Max size

							//LAND SIZE
							.ib-item-collapse
								h2.ib-header-collapse Land Size
								.ib-body-collpase
									.ib-wrap-fm
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="0") 0 Sq.Ft.
												option(value="---", disabled="") ---
												option(value="100") 100 Sq.Ft.
												option(value="200") 200 Sq.Ft.
												option(value="1000") 1000 Sq.Ft.
											span.ib-label-wrap-fm Min size
										.ib-item-wrap-fm.ib-sp-fm.ib-sp-fm-tp to
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="any") Any
												option(value="---", disabled="") ---
												option(value="100") 100 Sq.Ft.
												option(value="200") 200 Sq.Ft.
												option(value="1000") 1000 Sq.Ft.
											span.ib-label-wrap-fm Max size
							
							//YEAR BUILT
							.ib-item-collapse
								h2.ib-header-collapse Year Built
								.ib-body-collpase
									.ib-wrap-fm
										.ib-item-wrap-fm.ib-wrap-content-select
											select(id="ib-range-yearMin", name="minyear")
												option(value="any") Any
												option(value="---", disabled="") ---
												option(value="2020") 2020
												option(value="2019") 2019
												option(value="2018") 2018
												option(value="2017") 2017
												option(value="2016") 2016
												option(value="2015") 2015
